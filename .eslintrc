{
    "env": {
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 9
    },
    "plugins": [
        "security"
    ],
    "extends": [
        "plugin:security/recommended"
    ],
    "rules": {
        "comma-dangle": 0,
        "arrow-parens": 0,
        "padded-blocks": 0,
        "no-trailing-spaces": 0,
        "object-curly-spacing": 0,
        "new-cap": 0,
        "max-len": 0,
        "security/detect-child-process": 0
    }
}