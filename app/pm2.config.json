{
    "apps": [
        {
            "script": "./app-compiled/app.js",
            "interpreter_args": "--inspect-brk=0.0.0.0:5858",
            "watch": false,
            "instances": 1,
            "exec_mode": "fork",
            "name": "Pyrrha Boxproxy",
            "env": {
                "ENV": "PRODUCTION"
            },
            "max_restarts": 10,
            "log_file": "./logs/combined.outerr.log",
            "out_file": "./logs/out.log",
            "error_file": "./logs/err.log",
            "log_date_format": "YYYY-MM-DD HH:mm Z",
            "log_type": "json"
        }
    ]
}